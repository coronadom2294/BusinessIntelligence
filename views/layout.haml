    .container-fluid
      .row-fluid
        .span2
          thest
        .span10
          = yield

  %script{:src => "/javascript/jquery-1.7.1.min.js"}
  %script{:src => "/javascript/backbone-min.js"}
  %script{:src => "/javascript/d3.v2.js"}
  %script{:src => "/bootstrap/js/bootstrap.min.js"}
  %script{:src => "http://maps.googleapis.com/maps/api/js?sensor=true"}
  %script{:src => "/javascript/application.js"}
